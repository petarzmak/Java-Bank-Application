<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="coe528.project.DepositController">
   <children>
      <Label layoutX="39.0" layoutY="29.0" prefHeight="17.0" prefWidth="230.0" text="Welcome to Deposit">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Label>
      <Label layoutX="39.0" layoutY="85.0" prefHeight="17.0" prefWidth="440.0" text="Please enter the amount you would like to Deposit in the box below: " />
      <TextField fx:id="myAmount" layoutX="39.0" layoutY="120.0" promptText="Enter Amount" />
      <Label fx:id="myLabel" layoutX="39.0" layoutY="151.0" text="Label" textFill="#9e0000" />
      <Button layoutX="207.0" layoutY="120.0" mnemonicParsing="false" onAction="#depositMon" text="Enter" />
      <Button layoutX="28.0" layoutY="342.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="86.0" text="&lt; Back" />
      <MenuButton fx:id="myMenu" alignment="CENTER" layoutX="557.0" layoutY="28.0" minWidth="99.0" mnemonicParsing="false" text="MenuButton">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#logOut" text="Log Out" />
        </items>
      </MenuButton>
   </children>
</AnchorPane>
