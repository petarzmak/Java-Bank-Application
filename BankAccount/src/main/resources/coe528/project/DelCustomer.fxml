<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="coe528.project.DelCustomerController">
   <children>
      <Label contentDisplay="CENTER" layoutX="188.0" layoutY="86.0" text="Enter the username of the cusomter you would like to delete:" />
      <Label layoutX="222.0" layoutY="45.0" text="Welcome to Customer Deletion!">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Label>
      <Button layoutX="324.0" layoutY="157.0" mnemonicParsing="false" onAction="#delete" text="Delete" />
      <TextField fx:id="myTextField" layoutX="276.0" layoutY="115.0" promptText="Username" />
      <Label fx:id="myLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="234.0" layoutY="192.0" prefHeight="17.0" prefWidth="231.0" text=" " textFill="#9e0707" />
      <Button layoutX="29.0" layoutY="341.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="85.0" text="&lt; Back" />
      <MenuButton fx:id="myMenu" alignment="CENTER" layoutX="557.0" layoutY="28.0" minWidth="99.0" mnemonicParsing="false" text="MenuButton">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#logOut" text="Log Out" />
        </items>
      </MenuButton>
   </children>
</AnchorPane>
